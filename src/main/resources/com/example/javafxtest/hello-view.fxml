<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.ProgressBar?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>
<?import javafx.scene.layout.VBox?>

<VBox fx:id="box" alignment="CENTER" prefHeight="525.0" prefWidth="570.0" spacing="20.0" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/17" fx:controller="com.example.javafxtest.ButtonController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <HBox alignment="CENTER" prefHeight="0.0" prefWidth="418.0" translateY="-20.0">
      <children>
         <Label alignment="CENTER" contentDisplay="CENTER" prefHeight="15.0" prefWidth="90.0" text="ECONOMY" translateX="-30.0" />
         <Label alignment="CENTER" prefHeight="15.0" prefWidth="90.0" text="ENVIRONMENT" translateX="0" />
         <Label alignment="CENTER" prefHeight="15.0" prefWidth="90.0" text="SOCIAL" translateX="30.0" />
      </children>
   </HBox>
   <HBox alignment="CENTER" prefHeight="24.0" prefWidth="530.0" translateY="-40.0">
      <children>
         <ProgressBar fx:id="eco" prefHeight="15.0" prefWidth="90.0" progress="0.0" translateX="-30.0" />
         <ProgressBar fx:id="env" prefHeight="15.0" prefWidth="90.0" progress="0.0" translateX="0.0" />
         <ProgressBar fx:id="soc" prefHeight="15.0" prefWidth="90.0" progress="0.0" translateX="30.0" />
      </children>
   </HBox>
    <children>
        <ImageView fx:id="iv" fitHeight="228.0" fitWidth="463.0" pickOnBounds="true" preserveRatio="true" translateY="-40.0" />
        <Label fx:id="prompt" alignment="CENTER" prefHeight="81.0" prefWidth="507.0" text="Label" translateY="-40.0" />
        <ButtonBar prefHeight="40.0" prefWidth="200.0" translateY="-40.0">
            <buttons>
                <Button mnemonicParsing="false" onAction="#onYesButtonClick" prefHeight="40.0" prefWidth="278.0" text="Yes" translateX="-21.0" />
                <Button onAction="#onNoButtonClick" prefHeight="40.0" prefWidth="80.0" text="No" textAlignment="CENTER" />
            </buttons>
        </ButtonBar>
    </children>
</VBox>
